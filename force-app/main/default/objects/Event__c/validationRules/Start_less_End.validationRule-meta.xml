<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_less_End</fullName>
    <active>true</active>
    <errorConditionFormula>EndDateTime__c &lt; StartDateTime__c || EndDateTime__c  =  StartDateTime__c</errorConditionFormula>
    <errorMessage>The end event cannot be earlier or the same time then start</errorMessage>
</ValidationRule>
